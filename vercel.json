{
    "buildCommand": "pip install . && cd web && yarn build",
    "outputDirectory": "web/dist",
    "installCommand": "cd web && yarn",
    "functions": {
        "api/asgi.py": {
            "excludeFiles": "web/**/*"
        }
    },
    "routes": [
        {
            "src": "/groups",
            "dest": "api/asgi"
        },
        {
            "src": "/teachers",
            "dest": "api/asgi"
        },
        {
            "src": "/sources",
            "dest": "api/asgi"
        },
        {
            "src": "/.*\\.ics",
            "dest": "api/asgi"
        },
        {
            "src": "/docs",
            "dest": "api/asgi"
        },
        {
            "src": "/openapi.json",
            "dest": "api/asgi"
        }
    ]
}
