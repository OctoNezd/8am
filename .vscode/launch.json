    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch via NPM",
            "request": "launch",
            "runtimeArgs": ["run", "dev"],
            "runtimeExecutable": "npm",
            "skipFiles": ["<node_internals>/**"],
            "type": "node",
            "cwd": "${workspaceFolder}/web",
            "console": "integratedTerminal"
        },
        {
            "name": "Test ics generation",
            "type": "python",
            "request": "launch",
            "program": "dec_reader.py",
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "Python: FastAPI",
            "type": "python",
            "request": "launch",
            "module": "uvicorn",
            "args": [
                "sharaga_ics:app",
                "--reload",
                "--host",
                "0.0.0.0",
                "--reload-exclude",
                "web"
            ],
            "jinja": true,
            "justMyCode": true
        }
    ]
}
